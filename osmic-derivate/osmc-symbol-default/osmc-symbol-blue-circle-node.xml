<!--OSMC symbols blue_circle node background-->
	<rule e="node" k="osmc_background" v="blue_circle" zoom-min="15">
		<rule e="node" k="osmc_foreground" v="~">
			<symbol src="file:/osmc-symbols/osmcbg_blue_circle.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_dot">
			<symbol src="file:/osmc-symbols/blue_circle_red_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="yellow_dot">
			<symbol src="file:/osmc-symbols/blue_circle_yellow_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="blue_dot">
			<symbol src="file:/osmc-symbols/blue_circle_blue_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="white_dot">
			<symbol src="file:/osmc-symbols/blue_circle_white_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="hiker">
			<symbol src="file:/osmc-symbols/blue_circle_hiker.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="blue_triangle">
			<symbol src="file:/osmc-symbols/blue_circle_blue_triangle.png" />
		</rule>
	</rule>
