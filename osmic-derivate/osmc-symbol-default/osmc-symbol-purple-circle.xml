<!--OSMC symbols purple_circle background-->
	<rule e="way" k="osmc_background" v="purple_circle" zoom-min="15">
		<rule e="way" k="osmc_foreground" v="~">
			<lineSymbol src="file:/osmc-symbols/osmcbg_purple_circle.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="green_dot">
			<lineSymbol src="file:/osmc-symbols/purple_circle_green_dot.png" align-center="false" repeat="true" />
		</rule>
	</rule>
