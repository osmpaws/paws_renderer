<!--OSMC symbols black_frame node background-->
	<rule e="node" k="osmc_background" v="black_frame" zoom-min="15">
		<rule e="node" k="osmc_foreground" v="~">
			<symbol src="file:/osmc-symbols/osmcbg_black_frame.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="green_corner">
			<symbol src="file:/osmc-symbols/black_frame_green_corner.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="black_cross">
			<symbol src="file:/osmc-symbols/black_frame_black_cross.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="blue_dot">
			<symbol src="file:/osmc-symbols/black_frame_blue_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="hiker">
			<symbol src="file:/osmc-symbols/black_frame_hiker.png" />
		</rule>
	</rule>
