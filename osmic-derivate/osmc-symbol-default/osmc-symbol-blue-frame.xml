<!--OSMC symbols blue_frame background-->
	<rule e="way" k="osmc_background" v="blue_frame" zoom-min="15">
		<rule e="way" k="osmc_foreground" v="~">
			<lineSymbol src="file:/osmc-symbols/osmcbg_blue_frame.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="blue_bar">
			<lineSymbol src="file:/osmc-symbols/blue_frame_blue_bar.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="white_bar">
			<lineSymbol src="file:/osmc-symbols/blue_frame_white_bar.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="blue_bowl">
			<lineSymbol src="file:/osmc-symbols/blue_frame_blue_bowl.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="hiker">
			<lineSymbol src="file:/osmc-symbols/blue_frame_hiker.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="green_lower">
			<lineSymbol src="file:/osmc-symbols/blue_frame_green_lower.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="mine">
			<lineSymbol src="file:/osmc-symbols/blue_frame_mine.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="blue_pointer">
			<lineSymbol src="file:/osmc-symbols/blue_frame_blue_pointer.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="blue_slash">
			<lineSymbol src="file:/osmc-symbols/blue_frame_blue_slash.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="green_slash">
			<lineSymbol src="file:/osmc-symbols/blue_frame_green_slash.png" align-center="false" repeat="true" />
		</rule>
	</rule>
