<!--OSMC symbols black_round node background-->
	<rule e="node" k="osmc_background" v="black_round" zoom-min="15">
		<rule e="node" k="osmc_foreground" v="~">
			<symbol src="file:/osmc-symbols/osmcbg_black_round.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="green_circle">
			<symbol src="file:/osmc-symbols/black_round_green_circle.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_dot">
			<symbol src="file:/osmc-symbols/black_round_red_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="yellow_dot">
			<symbol src="file:/osmc-symbols/black_round_yellow_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="white_dot">
			<symbol src="file:/osmc-symbols/black_round_white_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="black_dot">
			<symbol src="file:/osmc-symbols/black_round_black_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="yellow_fork">
			<symbol src="file:/osmc-symbols/black_round_yellow_fork.png" />
		</rule>
	</rule>
