<!--OSMC symbols yellow_frame node background-->
	<rule e="node" k="osmc_background" v="yellow_frame" zoom-min="15">
		<rule e="node" k="osmc_foreground" v="~">
			<symbol src="file:/osmc-symbols/osmcbg_yellow_frame.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="yellow_bar">
			<symbol src="file:/osmc-symbols/yellow_frame_yellow_bar.png" />
