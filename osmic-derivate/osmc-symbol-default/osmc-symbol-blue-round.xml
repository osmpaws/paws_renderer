<!--OSMC symbols blue_round background-->
	<rule e="way" k="osmc_background" v="blue_round" zoom-min="15">
		<rule e="way" k="osmc_foreground" v="~">
			<lineSymbol src="file:/osmc-symbols/osmcbg_blue_round.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="blue_circle">
			<lineSymbol src="file:/osmc-symbols/blue_round_blue_circle.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="red_dot">
			<lineSymbol src="file:/osmc-symbols/blue_round_red_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="blue_dot">
			<lineSymbol src="file:/osmc-symbols/blue_round_blue_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="white_dot">
			<lineSymbol src="file:/osmc-symbols/blue_round_white_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="white_hiker">
			<lineSymbol src="file:/osmc-symbols/blue_round_white_hiker.png" align-center="false" repeat="true" />
		</rule>
	</rule>
