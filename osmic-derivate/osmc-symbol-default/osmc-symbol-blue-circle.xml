<!--OSMC symbols blue_circle background-->
	<rule e="way" k="osmc_background" v="blue_circle" zoom-min="15">
		<rule e="way" k="osmc_foreground" v="yellow_dot">
			<lineSymbol src="file:/osmc-symbols/blue_circle_yellow_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="blue_dot">
			<lineSymbol src="file:/osmc-symbols/blue_circle_blue_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="white_dot">
			<lineSymbol src="file:/osmc-symbols/blue_circle_white_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="hiker">
			<lineSymbol src="file:/osmc-symbols/blue_circle_hiker.png" align-center="false" repeat="true" />
		</rule>
	</rule>
