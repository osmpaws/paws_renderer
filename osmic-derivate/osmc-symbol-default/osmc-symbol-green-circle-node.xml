<!--OSMC symbols green_circle node background-->
	<rule e="node" k="osmc_background" v="green_circle" zoom-min="15">
		<rule e="node" k="osmc_foreground" v="~">
			<symbol src="file:/osmc-symbols/osmcbg_green_circle.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="blue_dot">
			<symbol src="file:/osmc-symbols/green_circle_blue_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="white_dot">
			<symbol src="file:/osmc-symbols/green_circle_white_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_lower">
			<symbol src="file:/osmc-symbols/green_circle_red_lower.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="green_triangle_turned">
			<symbol src="file:/osmc-symbols/green_circle_green_triangle_turned.png" />
		</rule>
	</rule>
