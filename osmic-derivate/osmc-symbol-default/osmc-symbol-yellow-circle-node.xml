<!--OSMC symbols yellow_circle node background-->
	<rule e="node" k="osmc_background" v="yellow_circle" zoom-min="15">
		<rule e="node" k="osmc_foreground" v="~">
			<symbol src="file:/osmc-symbols/osmcbg_yellow_circle.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="yellow_bar">
			<symbol src="file:/osmc-symbols/yellow_circle_yellow_bar.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="yellow_dot">
			<symbol src="file:/osmc-symbols/yellow_circle_yellow_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="green_dot">
			<symbol src="file:/osmc-symbols/yellow_circle_green_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="black_pointer">
			<symbol src="file:/osmc-symbols/yellow_circle_black_pointer.png" />
		</rule>
	</rule>
