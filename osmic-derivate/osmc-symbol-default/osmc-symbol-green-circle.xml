<!--OSMC symbols green_circle background-->
	<rule e="way" k="osmc_background" v="green_circle" zoom-min="15">
		<rule e="way" k="osmc_foreground" v="~">
			<lineSymbol src="file:/osmc-symbols/osmcbg_green_circle.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="blue_dot">
			<lineSymbol src="file:/osmc-symbols/green_circle_blue_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="white_dot">
			<lineSymbol src="file:/osmc-symbols/green_circle_white_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="red_lower">
			<lineSymbol src="file:/osmc-symbols/green_circle_red_lower.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="yellow_pointer">
			<lineSymbol src="file:/osmc-symbols/green_circle_yellow_pointer.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="green_triangle_turned">
			<lineSymbol src="file:/osmc-symbols/green_circle_green_triangle_turned.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="green_triangle">
			<lineSymbol src="file:/osmc-symbols/green_circle_green_triangle.png" align-center="false" repeat="true" />
		</rule>
	</rule>
