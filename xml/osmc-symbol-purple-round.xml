<!--OSMC symbols purple_round background-->
	<rule e="way" k="osmc_background" v="purple_round" zoom-min="15">
		<rule e="way" k="osmc_foreground" v="~">
			<lineSymbol src="file:/osmc-symbols/osmcbg_purple_round.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="yellow_dot">
			<lineSymbol src="file:/osmc-symbols/purple_round_yellow_dot.png" align-center="false" repeat="true" />
		</rule>
	</rule>
