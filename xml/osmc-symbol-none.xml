<!--OSMC symbols none background-->
	<rule e="way" k="osmc_background" v="none" zoom-min="15">
		<rule e="way" k="osmc_foreground" v="~">
			<lineSymbol src="file:/osmc-symbols/osmcbg_none.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="yellow_dot">
			<lineSymbol src="file:/osmc-symbols/none_yellow_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="red_pointer">
			<lineSymbol src="file:/osmc-symbols/none_red_pointer.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="blue_pointer">
			<lineSymbol src="file:/osmc-symbols/none_blue_pointer.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="red_triangle">
			<lineSymbol src="file:/osmc-symbols/none_red_triangle.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="green_triangle">
			<lineSymbol src="file:/osmc-symbols/none_green_triangle.png" align-center="false" repeat="true" />
		</rule>
	</rule>
