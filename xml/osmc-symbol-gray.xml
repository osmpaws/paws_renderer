<!--OSMC symbols gray background-->
	<rule e="way" k="osmc_background" v="gray" zoom-min="15">
		<rule e="way" k="osmc_foreground" v="~">
			<lineSymbol src="file:/osmc-symbols/osmcbg_gray.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="yellow_bar">
			<lineSymbol src="file:/osmc-symbols/gray_yellow_bar.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="blue_bar">
			<lineSymbol src="file:/osmc-symbols/gray_blue_bar.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="white_bar">
			<lineSymbol src="file:/osmc-symbols/gray_white_bar.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="black_bar">
			<lineSymbol src="file:/osmc-symbols/gray_black_bar.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="purple_bar">
			<lineSymbol src="file:/osmc-symbols/gray_purple_bar.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="orange_lower">
			<lineSymbol src="file:/osmc-symbols/gray_orange_lower.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="red_rectangle">
			<lineSymbol src="file:/osmc-symbols/gray_red_rectangle.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="white_stripe">
			<lineSymbol src="file:/osmc-symbols/gray_white_stripe.png" align-center="false" repeat="true" />
		</rule>
	</rule>
