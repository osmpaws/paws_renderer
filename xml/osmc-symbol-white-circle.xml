<!--OSMC symbols white_circle background-->
	<rule e="way" k="osmc_background" v="white_circle" zoom-min="15">
		<rule e="way" k="osmc_foreground" v="~">
			<lineSymbol src="file:/osmc-symbols/osmcbg_white_circle.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="red_circle">
			<lineSymbol src="file:/osmc-symbols/white_circle_red_circle.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="red_dot">
			<lineSymbol src="file:/osmc-symbols/white_circle_red_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="yellow_dot">
			<lineSymbol src="file:/osmc-symbols/white_circle_yellow_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="blue_dot">
			<lineSymbol src="file:/osmc-symbols/white_circle_blue_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="green_dot">
			<lineSymbol src="file:/osmc-symbols/white_circle_green_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="black_dot">
			<lineSymbol src="file:/osmc-symbols/white_circle_black_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="purple_dot">
			<lineSymbol src="file:/osmc-symbols/white_circle_purple_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="orange_dot">
			<lineSymbol src="file:/osmc-symbols/white_circle_orange_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="black_rectangle">
			<lineSymbol src="file:/osmc-symbols/white_circle_black_rectangle.png" align-center="false" repeat="true" />
		</rule>
	</rule>
