		<!--$KCT#biking#lines#high#zoom$-->
		<rule cat="bike_lines" e="way" k="route" v="bicycle">
			<rule e="way" k="network" v="ncn" zoom-min="19">
				<line stroke="#9000FF" stroke-width="1.5" dy="-3" stroke-linecap="butt" />
			</rule>
			<rule e="way" k="network" v="ncn" zoom-min="17" zoom-max="18">
				<line stroke="#9000FF" stroke-width="2.2" dy="-3" stroke-linecap="butt" />
			</rule>
			<rule e="way" k="network" v="rcn|lcn|~" zoom-min="19">
				<line stroke="#FF00FF" stroke-width="1.5" dy="-3" stroke-linecap="butt" />
			</rule>
			<rule e="way" k="network" v="rcn|lcn|~" zoom-min="17" zoom-max="18">
				<line stroke="#FF00FF" stroke-width="2.2" dy="-3" stroke-linecap="butt" />
			</rule>
		</rule>
