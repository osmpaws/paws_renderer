<!--OSMC symbols black_circle node background-->
	<rule e="node" k="osmc_background" v="black_circle" zoom-min="15">
		<rule e="node" k="osmc_foreground" v="~">
			<symbol src="file:/osmc-symbols/osmcbg_black_circle.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_bar">
			<symbol src="file:/osmc-symbols/black_circle_red_bar.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="green_bar">
			<symbol src="file:/osmc-symbols/black_circle_green_bar.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="black_bar">
			<symbol src="file:/osmc-symbols/black_circle_black_bar.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_dot">
			<symbol src="file:/osmc-symbols/black_circle_red_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="yellow_dot">
			<symbol src="file:/osmc-symbols/black_circle_yellow_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="blue_dot">
			<symbol src="file:/osmc-symbols/black_circle_blue_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="green_dot">
			<symbol src="file:/osmc-symbols/black_circle_green_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="white_dot">
			<symbol src="file:/osmc-symbols/black_circle_white_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="black_dot">
			<symbol src="file:/osmc-symbols/black_circle_black_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="brown_dot">
			<symbol src="file:/osmc-symbols/black_circle_brown_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="purple_dot">
			<symbol src="file:/osmc-symbols/black_circle_purple_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="orange_dot">
			<symbol src="file:/osmc-symbols/black_circle_orange_dot.png" />
		</rule>
	</rule>
