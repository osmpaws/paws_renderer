<!--KCT#biking#lines#low#zoom-->
		<rule cat="bike_lines" e="way" k="route" v="bicycle">
			<rule e="way" k="network" v="ncn" zoom-min="15" zoom-max="16">
				<line stroke="#9000FF" stroke-width="5.1" stroke-linecap="round" stroke-dasharray="0.15,75" />
			</rule>
			<rule e="way" k="network" v="ncn" zoom-min="13" zoom-max="14">
				<line stroke="#9000FF" stroke-width="6.0" stroke-linecap="round" stroke-dasharray="0.15,37.5" />
			</rule>
			<rule e="way" k="network" v="ncn" zoom-min="12" zoom-max="12">
				<line stroke="#9000FF" stroke-width="7.2" stroke-linecap="round" stroke-dasharray="0.15,15" />
			</rule>
			<!--rule e="way" k="network" v="ncn" zoom-min="11" zoom-max="11">
				<line stroke="#9000FF" stroke-width="4.5" stroke-linecap="round" stroke-dasharray="0.15,9" />
			</rule-->
			<rule e="way" k="network" v="rcn|lcn|~" zoom-min="15" zoom-max="16">
				<line stroke="#FF00FF" stroke-width="4.8" stroke-linecap="round" stroke-dasharray="0,7.5,0.15,75" />
			</rule>
			<rule e="way" k="network" v="rcn|lcn|~" zoom-min="13" zoom-max="14">
				<line stroke="#FF00FF" stroke-width="6.0" stroke-linecap="round" stroke-dasharray="0,7.5,0.15,37.5" />
			</rule>
			<rule e="way" k="network" v="rcn|lcn|~" zoom-min="12" zoom-max="12">
				<line stroke="#FF00FF" stroke-width="7.2" stroke-linecap="round" stroke-dasharray="0,7.5,0.15,15" />
			</rule>
			<!--rule e="way" k="network" v="rcn|lcn|~" zoom-min="11" zoom-max="11">
				<line stroke="#FF00FF" stroke-width="4.5" stroke-linecap="round" stroke-dasharray="0,7.5,0.15,9" />
			</rule-->
		</rule>
