		<!--forest-->
		<rule e="way" k="natural|landuse" v="forest|wood">
			<rule e="way" k="wood" v="*" zoom-min="11">
				<rule e="way" k="wood" v="coniferous">
					<area src="file:/patterns/wood_coniferous.png" stroke="#6fc18e" stroke-width="0.1" />
				</rule>
				<rule e="way" k="wood" v="deciduous">
					<area src="file:/patterns/wood_deciduous.png" stroke="#6fc18e" stroke-width="0.1" />
				</rule>
				<rule e="way" k="wood" v="mixed">
					<area src="file:/patterns/wood_mixed.png" stroke="#6fc18e" stroke-width="0.1" />
				</rule>
			</rule>
			<rule e="way" k="wood" v="~" zoom-max="10">
				<area fill="#c1cfba" />
			</rule>
			<rule e="way" k="wood" v="~" zoom-min="11">
				<area src="file:/patterns/wood_mixed.png" stroke="#6fc18e" stroke-width="0.1" />
				<!--area fill="#a6c993" stroke="#6fc18e" stroke-width="0.3" /-->
			</rule>

			<rule e="way" k="*" v="*" zoom-min="16">
				<caption k="name" font-style="bold" font-size="17.0" fill="#40aa40" stroke="#ffffff" stroke-width="3.0" />
			</rule>
		</rule>
