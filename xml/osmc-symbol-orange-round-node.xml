<!--OSMC symbols orange_round node background-->
	<rule e="node" k="osmc_background" v="orange_round" zoom-min="15">
		<rule e="node" k="osmc_foreground" v="~">
			<symbol src="file:/osmc-symbols/osmcbg_orange_round.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="white_dot">
			<symbol src="file:/osmc-symbols/orange_round_white_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="orange_dot">
			<symbol src="file:/osmc-symbols/orange_round_orange_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_pointer">
			<symbol src="file:/osmc-symbols/orange_round_red_pointer.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="yellow_pointer">
			<symbol src="file:/osmc-symbols/orange_round_yellow_pointer.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="blue_pointer">
			<symbol src="file:/osmc-symbols/orange_round_blue_pointer.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="green_pointer">
			<symbol src="file:/osmc-symbols/orange_round_green_pointer.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="brown_pointer">
			<symbol src="file:/osmc-symbols/orange_round_brown_pointer.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="purple_pointer">
			<symbol src="file:/osmc-symbols/orange_round_purple_pointer.png" />
		</rule>
	</rule>
