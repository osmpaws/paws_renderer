<!--OSMC symbols purple_circle node background-->
	<rule e="node" k="osmc_background" v="purple_circle" zoom-min="15">
		<rule e="node" k="osmc_foreground" v="~">
			<symbol src="file:/osmc-symbols/osmcbg_purple_circle.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="green_dot">
			<symbol src="file:/osmc-symbols/purple_circle_green_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="purple_triangle">
			<symbol src="file:/osmc-symbols/purple_circle_purple_triangle.png" />
		</rule>
	</rule>
