<!--OSMC symbols black_circle background-->
	<rule e="way" k="osmc_background" v="black_circle" zoom-min="15">
		<rule e="way" k="osmc_foreground" v="red_dot">
			<lineSymbol src="file:/osmc-symbols/black_circle_red_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="yellow_dot">
			<lineSymbol src="file:/osmc-symbols/black_circle_yellow_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="blue_dot">
			<lineSymbol src="file:/osmc-symbols/black_circle_blue_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="green_dot">
			<lineSymbol src="file:/osmc-symbols/black_circle_green_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="white_dot">
			<lineSymbol src="file:/osmc-symbols/black_circle_white_dot.png" align-center="false" repeat="true" />
		</rule>
	</rule>
