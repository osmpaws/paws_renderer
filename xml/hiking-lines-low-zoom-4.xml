		<!--hiking#lines#low#zoom-->
		<rule cat="hike_lines" e="way" k="route" v="hiking|foot|wheelchair|ski" zoom-min="12" zoom-max="13">
			<rule e="way" k="osmc_color" v="orange" >
				<line stroke="#FFFFFF" stroke-width="3.7" stroke-linecap="butt" />
				<line stroke="#F96F00" stroke-width="3.4" stroke-linecap="butt" />
			</rule>
			<rule e="way" k="osmc_color" v="purple" >
				<line stroke="#FFFFFF" stroke-width="3.7" stroke-linecap="butt" />
				<line stroke="#B878DD" stroke-width="3.4" stroke-linecap="butt" />
			</rule>
			<rule e="way" k="osmc_color" v="black" >
				<line stroke="#FFFFFF" stroke-width="3.7" stroke-linecap="butt" />
				<line stroke="#000000" stroke-width="3.4" stroke-linecap="butt" />
			</rule>
			<rule e="way" k="osmc_color" v="none" >
				<line stroke="#FFFFFF" stroke-width="3.7" stroke-linecap="butt" />
				<line stroke="#AAAAAA" stroke-width="3.4" stroke-linecap="butt" />
			</rule>
			<rule e="way" k="osmc_color" v="blue" >
				<line stroke="#FFFFFF" stroke-width="3.7" stroke-linecap="butt" />
				<line stroke="#1579e0" stroke-width="3.0" stroke-linecap="butt" />
			</rule>
			<rule e="way" k="osmc_color" v="yellow" >
				<line stroke="#CCCCCC" stroke-width="4.2" stroke-linecap="butt" />
				<line stroke="#FFDD00" stroke-width="3.0" stroke-linecap="butt" />
			</rule>
			<rule e="way" k="osmc_color" v="green" >
				<line stroke="#FFFFFF" stroke-width="3.7" stroke-linecap="butt" />
				<line stroke="#00cd27" stroke-width="3.0" stroke-linecap="butt" />
			</rule>
			<rule e="way" k="osmc_color" v="red" >
				<line stroke="#FFFFFF" stroke-width="3.7" stroke-linecap="butt" />
				<line stroke="#FF0000" stroke-width="3.0" stroke-linecap="butt" />
			</rule>
			<rule e="way" k="osmc_color" v="red" >
				<line stroke="#FF0000" stroke-width="3.0" stroke-linecap="butt" stroke-dasharray="10,70" />
			</rule>
			<rule e="way" k="osmc_color" v="green" >
				<line stroke="#00cd27" stroke-width="3.0" stroke-linecap="butt" stroke-dasharray="0,10,10,60" />
			</rule>
			<rule e="way" k="osmc_color" v="yellow" >
				<line stroke="#FFDD00" stroke-width="3.0" stroke-linecap="butt" stroke-dasharray="0,20,10,50" />
			</rule>
			<rule e="way" k="osmc_color" v="blue" >
				<line stroke="#1579e0" stroke-width="3.0" stroke-linecap="butt" stroke-dasharray="0,30,10,40" />
			</rule>
			<rule e="way" k="osmc_color" v="none" >
				<line stroke="#AAAAAA" stroke-width="3.0" stroke-linecap="butt" stroke-dasharray="0,40,10,30" />
			</rule>
			<rule e="way" k="osmc_color" v="black" >
				<line stroke="#000000" stroke-width="3.0" stroke-linecap="butt" stroke-dasharray="0,30,10,40" />
			</rule>
			<rule e="way" k="osmc_color" v="purple" >
				<line stroke="#B878DD" stroke-width="3.0" stroke-linecap="butt" stroke-dasharray="0,20,10,50" />
			</rule>
			<rule e="way" k="osmc_color" v="orange" >
				<line stroke="#F96F00" stroke-width="3.0" stroke-linecap="butt" stroke-dasharray="0,10,10,60" />
			</rule>
		</rule>
