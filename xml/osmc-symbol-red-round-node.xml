<!--OSMC symbols red_round node background-->
	<rule e="node" k="osmc_background" v="red_round" zoom-min="15">
		<rule e="node" k="osmc_foreground" v="~">
			<symbol src="file:/osmc-symbols/osmcbg_red_round.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="yellow_bowl">
			<symbol src="file:/osmc-symbols/red_round_yellow_bowl.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="black_circle">
			<symbol src="file:/osmc-symbols/red_round_black_circle.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_dot">
			<symbol src="file:/osmc-symbols/red_round_red_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="white_dot">
			<symbol src="file:/osmc-symbols/red_round_white_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_hiker">
			<symbol src="file:/osmc-symbols/red_round_red_hiker.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="white_hiker">
			<symbol src="file:/osmc-symbols/red_round_white_hiker.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="white_rectangle">
			<symbol src="file:/osmc-symbols/red_round_white_rectangle.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_triangle_line">
			<symbol src="file:/osmc-symbols/red_round_red_triangle_line.png" />
		</rule>
	</rule>
