		<!-- railway (no tunnel) -->
		<rule e="way" k="railway" v="*">
			<rule e="way" k="tunnel" v="~|false|no">
				<rule e="way" k="railway" v="station">
					<area fill="#9b9b79" stroke="#707070" stroke-width="0.4" />
				</rule>

				<!-- railway bridge casings -->
				<rule e="way" k="bridge" v="yes|true">
					<rule e="way" k="railway" v="tram">
						<line stroke="#000000" stroke-width="0.6" stroke-linecap="butt" />
					</rule>
					<rule e="way" k="railway" v="subway|light_rail|narrow_gauge|abandoned|preserved|disused">
						<line stroke="#000000" stroke-width="0.9" stroke-linecap="butt" />
					</rule>
					<rule e="way" k="railway" v="rail">
						<line stroke="#000000" stroke-width="1.2" stroke-linecap="butt" />
					</rule>
				</rule>

				<!-- railway casings and cores -->
				<rule e="way" k="railway" v="tram">
					<line stroke="#e6b7e6" stroke-width="0.37" stroke-linecap="butt" />
					<line stroke="#4c0f4c" stroke-width="0.37" stroke-dasharray="18,27" stroke-linecap="butt" />
				</rule>
				<rule e="way" k="railway" v="funicular">
					<line stroke="#e6b7e6" stroke-width="0.37" stroke-linecap="butt" />
					<line stroke="#b7e6b7" stroke-width="0.37" stroke-dasharray="18,27" stroke-linecap="butt" />
				</rule>
				<rule e="way" k="railway" v="subway">
					<line stroke="#0f0f4c" stroke-width="0.6" stroke-linecap="butt" />
					<line stroke="#b7b7e6" stroke-width="0.37" stroke-dasharray="18,27" stroke-linecap="butt" />
				</rule>
				<rule e="way" k="railway" v="light_rail">
					<line stroke="#0f4c0f" stroke-width="0.6" stroke-linecap="butt" />
					<line stroke="#b7e6e6" stroke-width="0.37" stroke-linecap="butt" />
					<line stroke="#0f4c0f" stroke-width="0.37" stroke-dasharray="18,27" stroke-linecap="butt" />
				</rule>
				<rule e="way" k="railway" v="abandoned|preserved|construction">
					<line stroke="#9f9f9f" stroke-width="0.6" stroke-linecap="butt" />
					<line stroke="#e6e6e6" stroke-width="0.37" stroke-linecap="butt" />
					<line stroke="#9f9f9f" stroke-width="0.37" stroke-dasharray="18,27" stroke-linecap="butt" />
				</rule>
				<rule e="way" k="railway" v="disused">
					<line stroke="#777777" stroke-width="0.6" stroke-linecap="butt" />
					<line stroke="#e6e6e6" stroke-width="0.37" stroke-linecap="butt" />
					<line stroke="#777777" stroke-width="0.37" stroke-dasharray="18,27" stroke-linecap="butt" />
				</rule>
				<rule e="way" k="railway" v="narrow_gauge">
					<line stroke="#333333" stroke-width="0.6" stroke-linecap="butt" />
					<line stroke="#e6e6e6" stroke-width="0.37" stroke-linecap="butt" />
					<line stroke="#333333" stroke-width="0.37" stroke-dasharray="27,27" stroke-linecap="butt" />
				</rule>
				<rule e="way" k="railway" v="rail" zoom-max="13">
					<rule e="way" k="service" v="~">
						<line stroke="#333333" stroke-width="1.5" stroke-linecap="butt" />
						<line stroke="#e6e6e6" stroke-width="1.3" stroke-linecap="butt" />
						<line stroke="#333333" stroke-width="1.3" stroke-dasharray="7.5,7.5" stroke-linecap="butt" />
					</rule>
				</rule>
				<rule e="way" k="railway" v="rail" zoom-min="14" zoom-max="16">
					<rule e="way" k="service" v="~">
						<line stroke="#333333" stroke-width="1.2" stroke-linecap="butt" />
						<line stroke="#e6e6e6" stroke-width="1.0" stroke-linecap="butt" />
						<line stroke="#333333" stroke-width="1.0" stroke-dasharray="22.5,22.5" stroke-linecap="butt" />
					</rule>
				</rule>
				<rule e="way" k="railway" v="rail" zoom-min="16">
					<rule e="way" k="service" v="~">
						<line stroke="#333333" stroke-width="0.8" stroke-linecap="butt" />
						<line stroke="#e6e6e6" stroke-width="0.6" stroke-linecap="butt" />
						<line stroke="#333333" stroke-width="0.6" stroke-dasharray="22.5,22.5" stroke-linecap="butt" />
					</rule>
				</rule>
				<rule e="way" k="railway" v="rail" zoom-min="16">
					<rule e="way" k="service" v="*">
						<line stroke="#0f4c0f" stroke-width="0.6" stroke-linecap="butt" />
						<line stroke="#b7e6e6" stroke-width="0.37" stroke-linecap="butt" />
						<line stroke="#0f4c0f" stroke-width="0.37" stroke-dasharray="18,27" stroke-linecap="butt" />
					</rule>
				</rule>
			</rule>
		</rule>
