<!--OSMC symbols red_frame background-->
	<rule e="way" k="osmc_background" v="red_frame" zoom-min="15">
		<rule e="way" k="osmc_foreground" v="red_arch">
			<lineSymbol src="file:/osmc-symbols/red_frame_red_arch.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="white_bar">
			<lineSymbol src="file:/osmc-symbols/red_frame_white_bar.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="red_turned_T">
			<lineSymbol src="file:/osmc-symbols/red_frame_red_turned_t.png" align-center="false" repeat="true" />
		</rule>
	</rule>
