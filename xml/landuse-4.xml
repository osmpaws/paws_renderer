		<!-- landuse -->
		<rule cat="not_car_mode" e="way" k="landuse" v="*">
			<rule e="way" k="landuse" v="residential">
				<area fill="#e4e4e4" />
			</rule>
			<rule e="way" k="landuse" v="retail">
				<area fill="#ffebeb" stroke="#e4e4e4" stroke-width="0.3" />
			</rule>
			<rule e="way" k="landuse" v="industrial|brownfield|railway" zoom-min="13">
				<area fill="#ecd8ff" stroke="#e4e4e4" stroke-width="0.3" />
			</rule>
			<rule e="way" k="landuse" v="commercial">
				<area fill="#ffffc0" stroke="#e4e4e4" stroke-width="0.3" />
			</rule>
			<rule e="way" k="landuse" v="construction|greenfield">
				<area fill="#a47c41" stroke="#e4e4e4" stroke-width="0.3" />
			</rule>
			<rule e="way" k="landuse" v="garages">
				<area fill="#d6d6e4" />
			</rule>
			<rule e="way" k="landuse" v="landfill|quarry">
				<area fill="#e9dd72" stroke="#556b2f" stroke-width="0.3" />
			</rule>
			<rule e="way" k="landuse" v="cemetery" zoom-min="13">
				<area src="file:/patterns/cemetery.png" stroke="#e4e4e4" stroke-width="0.3" />
				<rule e="way" k="religion" v="christian" zoom-min="13">
					<area src="file:/patterns/cemetery_christian.png" stroke="#e4e4e4" stroke-width="0.3" />
				</rule>
				<rule e="way" k="religion" v="jewish" zoom-min="13">
					<area src="file:/patterns/cemetery_jewish.png" stroke="#e4e4e4" stroke-width="0.3" />
				</rule>
			</rule>
			<rule e="way" k="landuse" v="field|farm|farmland" zoom-min="13">
				<area fill="#FFF5ED" stroke="#cccc88" stroke-width="0.1" />
			</rule>
			<rule e="way" k="landuse" v="farmyard" zoom-min="13">
				<area fill="#cc9933" stroke="#ddbb77" stroke-width="0.1" />
			</rule>
			<rule e="way" k="landuse" v="meadow|grass" zoom-min="13">
				<area fill="#EDFBE0" stroke="#77dd66" stroke-width="0.1" />
			</rule>
			<rule e="way" k="landuse" v="allotments|village_green|recreation_ground" zoom-min="13">
				<area fill="#bde3cb" stroke="#6fc13d" stroke-width="0.3" />
			</rule>
			<rule e="way" k="landuse" v="plant_nursery" zoom-min="13">
				<area src="file:/patterns/plant_nursery.png" stroke="#6fc18e" stroke-width="0.3" />
			</rule>
			<rule e="way" k="landuse" v="orchard" zoom-min="13">
				<area src="file:/patterns/orchard.png" stroke="#6fc18e" stroke-width="0.3" />
			</rule>
			<rule e="way" k="landuse" v="vineyard" zoom-min="13">
				<area src="file:/patterns/vineyard.png" stroke="#6fc18e" stroke-width="0.3" />
			</rule>
			<rule e="way" k="landuse" v="military">
				<area src="file:/patterns/military.png" stroke="#e4e4e4" stroke-width="0.3" />
				<rule e="way" k="*" v="*" zoom-min="16">
					<caption k="name" font-style="bold" font-size="17.0" fill="#408040" stroke="#ffffff" stroke-width="3.0" />
				</rule>
			</rule>
			<rule e="way" k="*" v="*" zoom-min="17">
				<caption k="name" font-style="bold" font-size="17.0" fill="#408040" stroke="#ffffff" stroke-width="3.0" />
			</rule>
		</rule>
