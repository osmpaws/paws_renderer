			<!--#guidepost#-->
			<rule cat="guidepost" e="node" k="information" v="guidepost">
				<rule e="node" k="*" v="*" zoom-min="14">
					<rule e="node" k="fody" v="~">
						<symbol src="file:/symbols/guidepost.png" priority="3" />
					</rule>
					<rule e="node" k="fody" v="bad">
						<symbol src="file:/symbols/guidepost_bad.png" priority="3" />
					</rule>
					<caption k="ele" dy="33.0" font-family="sans_serif" font-style="italic" font-size="17" fill="#000000" stroke="#FFFFFF" stroke-width="3.7" />
				</rule>
			</rule>
