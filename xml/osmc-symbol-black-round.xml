<!--OSMC symbols black_round background-->
	<rule e="way" k="osmc_background" v="black_round" zoom-min="15">
		<rule e="way" k="osmc_foreground" v="~">
			<lineSymbol src="file:/osmc-symbols/osmcbg_black_round.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="red_dot">
			<lineSymbol src="file:/osmc-symbols/black_round_red_dot.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="white_dot">
			<lineSymbol src="file:/osmc-symbols/black_round_white_dot.png" align-center="false" repeat="true" />
		</rule>
	</rule>
