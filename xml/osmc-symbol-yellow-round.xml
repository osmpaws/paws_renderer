<!--OSMC symbols yellow_round background-->
	<rule e="way" k="osmc_background" v="yellow_round" zoom-min="15">
		<rule e="way" k="osmc_foreground" v="~">
			<lineSymbol src="file:/osmc-symbols/osmcbg_yellow_round.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="green_bowl">
			<lineSymbol src="file:/osmc-symbols/yellow_round_green_bowl.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="green_circle">
			<lineSymbol src="file:/osmc-symbols/yellow_round_green_circle.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="yellow_dot">
			<lineSymbol src="file:/osmc-symbols/yellow_round_yellow_dot.png" align-center="false" repeat="true" />
		</rule>
	</rule>
