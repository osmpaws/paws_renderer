<!--OSMC symbols none node background-->
	<rule e="node" k="osmc_background" v="none" zoom-min="15">
		<rule e="node" k="osmc_foreground" v="~">
			<symbol src="file:/osmc-symbols/osmcbg_none.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="yellow_diamond">
			<symbol src="file:/osmc-symbols/none_yellow_diamond.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="yellow_dot">
			<symbol src="file:/osmc-symbols/none_yellow_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_pointer">
			<symbol src="file:/osmc-symbols/none_red_pointer.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="blue_pointer">
			<symbol src="file:/osmc-symbols/none_blue_pointer.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="green_pointer">
			<symbol src="file:/osmc-symbols/none_green_pointer.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_triangle">
			<symbol src="file:/osmc-symbols/none_red_triangle.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="green_triangle">
			<symbol src="file:/osmc-symbols/none_green_triangle.png" />
		</rule>
	</rule>
