<!--OSMC symbols blue_round node background-->
	<rule e="node" k="osmc_background" v="blue_round" zoom-min="15">
		<rule e="node" k="osmc_foreground" v="~">
			<symbol src="file:/osmc-symbols/osmcbg_blue_round.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="blue_circle">
			<symbol src="file:/osmc-symbols/blue_round_blue_circle.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_dot">
			<symbol src="file:/osmc-symbols/blue_round_red_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="blue_dot">
			<symbol src="file:/osmc-symbols/blue_round_blue_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="white_dot">
			<symbol src="file:/osmc-symbols/blue_round_white_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="white_hiker">
			<symbol src="file:/osmc-symbols/blue_round_white_hiker.png" />
		</rule>
	</rule>
