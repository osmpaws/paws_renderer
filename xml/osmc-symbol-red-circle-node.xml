<!--OSMC symbols red_circle node background-->
	<rule e="node" k="osmc_background" v="red_circle" zoom-min="15">
		<rule e="node" k="osmc_foreground" v="~">
			<symbol src="file:/osmc-symbols/osmcbg_red_circle.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_bar">
			<symbol src="file:/osmc-symbols/red_circle_red_bar.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="white_circle">
			<symbol src="file:/osmc-symbols/red_circle_white_circle.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_cross">
			<symbol src="file:/osmc-symbols/red_circle_red_cross.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_dot">
			<symbol src="file:/osmc-symbols/red_circle_red_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="yellow_dot">
			<symbol src="file:/osmc-symbols/red_circle_yellow_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="white_dot">
			<symbol src="file:/osmc-symbols/red_circle_white_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_fork">
			<symbol src="file:/osmc-symbols/red_circle_red_fork.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="hiker">
			<symbol src="file:/osmc-symbols/red_circle_hiker.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_triangle_turned">
			<symbol src="file:/osmc-symbols/red_circle_red_triangle_turned.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="red_triangle">
			<symbol src="file:/osmc-symbols/red_circle_red_triangle.png" />
		</rule>
	</rule>
