		<!--KCT#hiking#lines#high#zoom-->
		<!--rule cat="hike_lines" e="way" k="highway" v="track|path|footway|steps|~" zoom-min="14"-->
		<rule cat="osmc_lines" e="way" k="*" v="*" zoom-min="14">
			<rule e="way" k="osmc_color" v="~">
				<rule e="way" k="network" v="lwn">
					<line stroke="#8B4513" stroke-width="1" stroke-linecap="butt" dy="1.5" />
				</rule>
				<rule e="way" k="network" v="rwn">
					<line stroke="#800000" stroke-width="1" stroke-linecap="butt" dy="1.5" />
				</rule>
				<rule e="way" k="network" v="nwn">
					<line stroke="#BC6F6F" stroke-width="1" stroke-linecap="butt" dy="-1.5" />
				</rule>
				<rule e="way" k="network" v="iwn">
					<line stroke="#D2691E" stroke-width="1" stroke-linecap="butt" dy="-1.5" />
				</rule>
			</rule>
			<rule e="way" k="osmc_color" v="white">
				<line stroke="#FFFFFF" stroke-width="1" stroke-linecap="butt" dy="-1.5" />
			</rule>
			<rule e="way" k="osmc_color" v="red">
				<line stroke="#FF0000" stroke-width="1" stroke-linecap="butt" dy="-1.5" />
			</rule>
			<rule e="way" k="osmc_color" v="blue">
				<line stroke="#1579e0" stroke-width="1" stroke-linecap="butt" dy="-1.5" />
			</rule>
			<rule e="way" k="osmc_color" v="purple">
				<line stroke="#B878DD" stroke-width="1" stroke-linecap="butt" dy="-1.5" />
			</rule>
			<rule e="way" k="osmc_color" v="white">
				<line stroke="#FFFFFF" stroke-width="1" stroke-linecap="butt" dy="-1.5" stroke-dasharray="5,10,5,0" />
			</rule>
			<rule e="way" k="osmc_color" v="purple">
				<line stroke="#B878DD" stroke-width="1" stroke-linecap="butt" dy="-1.5" stroke-dasharray="0,20,10" />
			</rule>
			<rule e="way" k="osmc_color" v="white">
				<line stroke="#FFFFFF" stroke-width="1" stroke-linecap="butt" dy="-1.5" stroke-dasharray="10,10" />
			</rule>
			<rule e="way" k="osmc_color" v="blue">
				<line stroke="#1579e0" stroke-width="1" stroke-linecap="butt" dy="-1.5" stroke-dasharray="10,10" />
			</rule>
			<rule e="way" k="osmc_color" v="red">
				<line stroke="#FF0000" stroke-width="1" stroke-linecap="butt" dy="-1.5" stroke-dasharray="5,10,5,0" />
			</rule>
			<rule e="way" k="osmc_color" v="white">
				<line stroke="#FFFFFF" stroke-width="1" stroke-linecap="butt" dy="-1.5" stroke-dasharray="0,15,10,5" />
			</rule>
			<rule e="way" k="osmc_color" v="yellow">
				<line stroke="#FFDD00" stroke-width="1" stroke-linecap="butt" dy="1.5" />
			</rule>
			<rule e="way" k="osmc_color" v="black">
				<line stroke="#000000" stroke-width="1" stroke-linecap="butt" dy="1.5" />
			</rule>
			<rule e="way" k="osmc_color" v="orange">
				<line stroke="#F96F00" stroke-width="1" stroke-linecap="butt" dy="1.5" />
			</rule>
			<rule e="way" k="osmc_color" v="green">
				<line stroke="#00cd27" stroke-width="1" stroke-linecap="butt" dy="1.5" />
				<!--line stroke="#00cd27" stroke-width="1" stroke-linecap="butt" dy="1.5" stroke-dasharray="10,10" /-->
			</rule>
			<rule e="way" k="osmc_color" v="orange">
				<line stroke="#F96F00" stroke-width="1" stroke-linecap="butt" dy="1.5" stroke-dasharray="5,10,5,0" />
			</rule>
			<rule e="way" k="osmc_color" v="black">
				<line stroke="#000000" stroke-width="1" stroke-linecap="butt" dy="1.5" stroke-dasharray="0,15,10" />
			</rule>
			<rule e="way" k="osmc_color" v="yellow">
				<line stroke="#FFDD00" stroke-width="1" stroke-linecap="butt" dy="1.5" stroke-dasharray="5,10,5,0" />
			</rule>
			<rule e="way" k="osmc_color" v="~">
				<rule e="way" k="network" v="lwn">
					<line stroke="#8B4513" stroke-width="1" stroke-linecap="butt" dy="1.5" stroke-dasharray="5,10,5,0" />
				</rule>
				<rule e="way" k="network" v="rwn">
					<line stroke="#800000" stroke-width="1" stroke-linecap="butt" dy="1.5" stroke-dasharray="10,30" />
				</rule>
				<rule e="way" k="network" v="nwn">
					<line stroke="#BC8F8F" stroke-width="1" stroke-linecap="butt" dy="-1.5" stroke-dasharray="5,10,5,0" />
				</rule>
				<rule e="way" k="network" v="iwn">
					<line stroke="#D2691E" stroke-width="1" stroke-linecap="butt" dy="-1.5" stroke-dasharray="10,30" />
				</rule>
			</rule>
		</rule>
		
