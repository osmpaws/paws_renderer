<!--OSMC symbols black_frame background-->
	<rule e="way" k="osmc_background" v="black_frame" zoom-min="15">
		<rule e="way" k="osmc_foreground" v="~">
			<lineSymbol src="file:/osmc-symbols/osmcbg_black_frame.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="green_corner">
			<lineSymbol src="file:/osmc-symbols/black_frame_green_corner.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="black_cross">
			<lineSymbol src="file:/osmc-symbols/black_frame_black_cross.png" align-center="false" repeat="true" />
		</rule>
		<rule e="way" k="osmc_foreground" v="blue_dot">
			<lineSymbol src="file:/osmc-symbols/black_frame_blue_dot.png" align-center="false" repeat="true" />
		</rule>
	</rule>
