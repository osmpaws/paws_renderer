<!--OSMC symbols purple_round node background-->
	<rule e="node" k="osmc_background" v="purple_round" zoom-min="15">
		<rule e="node" k="osmc_foreground" v="~">
			<symbol src="file:/osmc-symbols/osmcbg_purple_round.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="yellow_dot">
			<symbol src="file:/osmc-symbols/purple_round_yellow_dot.png" />
		</rule>
		<rule e="node" k="osmc_foreground" v="white_pointer">
			<symbol src="file:/osmc-symbols/purple_round_white_pointer.png" />
		</rule>
	</rule>
