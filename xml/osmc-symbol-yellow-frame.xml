<!--OSMC symbols yellow_frame background-->
	<rule e="way" k="osmc_background" v="yellow_frame" zoom-min="15">
		<rule e="way" k="osmc_foreground" v="~">
			<lineSymbol src="file:/osmc-symbols/osmcbg_yellow_frame.png" align-center="false" repeat="true" />
		</rule>
